{
    "manifest_version": 3,
    "name": "VK filter",
    "version": "1.1.0",
    "description": "VK - фильтр",
    "icons": {"128": "icon_128.png"},

    "permissions": [
        "storage"
    ],

    "content_scripts": [
        {
            "matches": [ "https://vk.com/*" ],
            "js": [ "app.js" ]
        }
    ]
}